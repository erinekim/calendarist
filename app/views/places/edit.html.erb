<div class="big-box col-xs-10 col-xs-offset-1">
	<h1>Edit Your Experience</h1>
	<%= simple_form_for @place do |f| %>
	<%= f.input :name %>
	<%= f.input :address %>
	<%= f.input :perfect_for %>
	<%= f.input :description %>
	<br />
	<%= f.submit 'Update', :class => 'btn btn-primary' %>
	<% end %>
</div>